{
  "runtime": "Default",
  "documentGenerator": {
    "aspNetCoreToOpenApi": {
      "assemblyPaths": [
        "../Scani.Api/bin/$(Configuration)/net6.0/Scani.Api.dll"
      ]
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "clientBaseClass": null,
      "generateClientClasses": true,
      "generateClientInterfaces": true,
      "clientBaseInterface": null,
      "injectHttpClient": true,
      "disposeHttpClient": false,
      "namespace": "Scani.Api.Client",
      "output": "ScaniApiClient.g.cs"
    }
  }
}